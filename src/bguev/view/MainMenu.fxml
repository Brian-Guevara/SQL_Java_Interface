<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="451.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bguev.view.MainMenuController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="403.0" prefWidth="601.0" spacing="30.0">
         <children>
            <Label fx:id="title" text="Main Menu">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <Label text="Signed in as: ">
                     <font>
                        <Font size="25.0" />
                     </font></Label>
                  <Label fx:id="userLabel" prefHeight="36.0" prefWidth="163.0" text="user">
                     <font>
                        <Font size="25.0" />
                     </font></Label>
               </children>
            </HBox>
            <Button minWidth="112.0" mnemonicParsing="false" onAction="#customers_pressed" text="Customers" />
            <Button minWidth="112.0" mnemonicParsing="false" onAction="#calendar_press" text="Calendar" />
            <Button minWidth="112.0" mnemonicParsing="false" onAction="#reports_press" text="Reports Page" />
            <Button minWidth="112.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
