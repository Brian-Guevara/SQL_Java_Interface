<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bguev.view.LoginMenuController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="11.0" layoutY="8.0" prefHeight="348.0" prefWidth="455.0" spacing="20.0">
         <children>
            <Label fx:id="title" alignment="CENTER" prefHeight="55.0" prefWidth="120.0" text="Title" />
            <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="408.0" spacing="75.0">
               <children>
                  <Label fx:id="username_text" text="Username:" />
                  <TextField fx:id="username" onAction="#sign_in_pressed" />
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="408.0" spacing="75.0">
               <children>
                  <Label fx:id="password_text" text="Password:" />
                  <TextField fx:id="password" onAction="#sign_in_pressed" />
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="50.0" prefWidth="408.0" spacing="50.0">
               <children>
                  <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#cancel_pressed" text="Cancel" />
                  <Button fx:id="signInBtn" mnemonicParsing="false" onAction="#sign_in_pressed" text="Sign In" />
               </children>
            </HBox>
            <Label fx:id="msg_label" text="MSG" />
         </children>
      </VBox>
   </children>
</AnchorPane>
